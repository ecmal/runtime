#!/usr/bin/env node 
try {
    require(process.cwd()+'/node_modules/@ecmal/runtime')
}catch(ex){
    require('@ecmal/runtime')
}
System.import(process.argv[2]).then(
    m=>{
        if(typeof (m.main)=='function'){
            m.main(process.argv.slice(2))
        }
    },
    e=>console.error(e)
);